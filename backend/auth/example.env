# Authentication Provider
# Possible values: oauth2-auth-code, oauth2-implicit, cookie-redirect
AUTH_PROVIDER=oauth2-auth-code

# JWT Configuration
DECODE_KEY=your_decode_key_here
# Possible values: HS256, HS384, HS512, RS256, RS384, RS512, ES256, ES384, ES512, PS256, PS384, PS512
JWT_ALGORITHM=HS256

# OAuth2 Configuration
OAUTH_CLIENT_ID=your_client_id_here
OAUTH_CLIENT_SECRET=your_client_secret_here
OAUTH_TOKEN_URL=https://your-oauth-token-url.com

# URLs
AUTH_LOGIN_URL=https://your-auth-login-url.com
FRONTEND_URI=https://your-frontend-uri.com
